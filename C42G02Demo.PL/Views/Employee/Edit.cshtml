@model EmployeeViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Employee</h1>

<br />
<br />

<form asp-action="Edit" enctype="multipart/form-data">
    
    <partial name="PartialViews/CreateEditEmployeePartial"></partial>

    <div>
